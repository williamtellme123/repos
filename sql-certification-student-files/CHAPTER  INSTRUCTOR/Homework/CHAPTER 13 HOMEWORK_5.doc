-- 10. Some simple examples of escaping a meta character "." the period
-- ------------------------------------------------------------------------------------------------------------
-- a. escaping a metacharacter
select regexp_substr('A.B*c','[.]') from dual;
-- ------------------------------------------------------------------------------------------------------------
select regexp_substr('A.B^c','(.)') from dual;
-- ------------------------------------------------------------------------------------------------------------
select regexp_substr('A.B^c','\.') from dual;
