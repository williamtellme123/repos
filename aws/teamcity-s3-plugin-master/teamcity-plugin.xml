<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>awsS3Plugin</name>
    <display-name>awsS3Plugin</display-name>
    <version>@Version@</version>
    <description>Uploads content to Amazon S3 buckets</description>
    <download-url>https://github.com/gonzadocarmo/teamcity-s3-plugin</download-url>
    <email>gonzalo.docarmo@gmail.com</email>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
      <logo>Plugin vendor logo URL</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
